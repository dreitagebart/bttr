{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "generate": {
      "cache": false
    },
    "gui": {
      "cache": false
    },
    "server": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "docker:dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
